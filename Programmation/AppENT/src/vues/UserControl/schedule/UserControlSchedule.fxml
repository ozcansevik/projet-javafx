<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root fx:id="maGrid" alignment="CENTER" prefHeight="400.0" prefWidth="600.0" type="GridPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="80.0" />
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="ALWAYS" />
      <RowConstraints />
  </rowConstraints>
  <ComboBox fx:id="groupSelected" GridPane.columnIndex="0" GridPane.rowIndex="0" />
      
   <Text text="Lundi" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
   <Text text="Mardi" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
   <Text text="Mercredi" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
   <Text text="Jeudi" GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
   <Text text="Vendredi" GridPane.columnIndex="5" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
   <Text text="Samedi" GridPane.columnIndex="6" GridPane.halignment="CENTER" GridPane.rowIndex="0" />
   <Text text="8H" GridPane.columnIndex="0" GridPane.rowIndex="1" />
   <Text text="10H" GridPane.columnIndex="0" GridPane.rowIndex="2" />
   <Text text="12H" GridPane.columnIndex="0" GridPane.rowIndex="3" />
   <Text text="13H30" GridPane.columnIndex="0" GridPane.rowIndex="4" />
   <Text text="15H30" GridPane.columnIndex="0" GridPane.rowIndex="5" />
   <Text text="17H30" GridPane.columnIndex="0" GridPane.rowIndex="6" />
   <Text text="19H30" GridPane.columnIndex="0" GridPane.rowIndex="7" />
   <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="8">
      <children>
         
         <Button mnemonicParsing="false" onAction="#clickOnPreviousDate" text="&lt;" wrapText="true">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin>
         </Button>
         <Text text="Semaine du">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin>
         </Text>
         <Text fx:id="debutSemaine" text="XX">
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Text>  <!--Binding sur date -->
         <Text text="au">
            <HBox.margin>
               <Insets left="10.0" right="10.0" />
            </HBox.margin>
         </Text>
         <Text fx:id="finSemaine" text="XX" />  <!--Binding sur date -->
         <Button mnemonicParsing="false" onAction="#clickOnNextDate" text="&gt;" wrapText="true">
            <HBox.margin>
               <Insets left="10.0" />
            </HBox.margin>
         </Button> 
      </children>
      <GridPane.margin>
         <Insets />
      </GridPane.margin>
   </HBox>
   <Text fx:id="TextMod" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="8" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS"/>
   <Button fx:id="buttonEdit" alignment="CENTER" mnemonicParsing="false" onAction="#clickOnEdit" text="Edition" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="8" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS" />
   <Button fx:id="buttonDelete" alignment="CENTER" mnemonicParsing="false" onAction="#clickOnDelete" text="Suppression" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="8" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS" />
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>

   
   
  
  
</fx:root>
